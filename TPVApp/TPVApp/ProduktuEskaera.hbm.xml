<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="TPVApp" namespace="TPVApp.Dominio">
	<class name="ProduktuEskaera" table="eskaeraproduktua">
		<id name="Id" column="id">
			<generator class="identity" />
		</id>
		<property name="Eskaera_id" column="eskaera_id" />
		<property name="ProduktuaIzena" column="produktua_izena" />
		<property name="ProduktuaKop" column="produktua_kop" />
		<property name="Prezioa" column="prezioa" />
		<many-to-one name="Eskaera" class="Eskaera" column="eskaera_id" />
	</class>
</hibernate-mapping>
